{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@anedomansky/ngx-form-fields": {
      "root": "projects/anedomansky/ngx-form-fields",
      "sourceRoot": "projects/anedomansky/ngx-form-fields/src",
      "prefix": "ngx-form",
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "changeDetection": "OnPush",
          "standalone": true
        },
        "@schematics/angular:directive": {
          "standalone": true
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/anedomansky/ngx-form-fields/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/anedomansky/ngx-form-fields/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/anedomansky/ngx-form-fields/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "test-app": {
      "root": "projects/test-app",
      "sourceRoot": "projects/test-app/src",
      "prefix": "app",
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "changeDetection": "OnPush",
          "standalone": true
        },
        "@schematics/angular:directive": {
          "standalone": true
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser-esbuild",
          "options": {
            "outputPath": "dist/test-app",
            "index": "projects/test-app/src/index.html",
            "main": "projects/test-app/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/test-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/test-app/src/favicon.ico",
              "projects/test-app/src/assets",
              {
                "glob": "**/*",
                "input": "dist/anedomansky/ngx-form-fields/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "projects/test-app/src/styles.scss",
              "dist/anedomansky/ngx-form-fields/styles/index.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1500kb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all",
              "aot": true,
              "buildOptimizer": true,
              "sourceMap": false,
              "namedChunks": false,
              "optimization": true
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "test-app:build:production"
            },
            "development": {
              "buildTarget": "test-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  }
}
