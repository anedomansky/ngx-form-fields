    "build:library": "ng build @anedomansky/ngx-form-group --configuration=production",
    "build:library:watch": "ng build @anedomansky/ngx-form-group --configuration=development --watch",
    "watch": "ng build --watch --configuration development",
    "test": "jest --silent",
    "test:local": "jest",
    "test:lib": "jest --silent --config ./jest.lib.config.js",
    "test:app": "jest --silent --config ./jest.app.config.js",
    "test:coverage": "jest --silent --collectCoverage --config ./jest.lib.config.js",
    "lint": "eslint ./projects/anedomansky/ngx-form-group --ext .ts --ext .html",
    "lint:fix": "eslint ./projects/anedomansky/ngx-form-group --ext .ts --ext .html --fix",
    "lint:scss": "stylelint \"projects/anedomansky/ngx-form-group/**/*.scss\"",
    "lint:scss:fix": "stylelint \"projects/anedomansky/ngx-form-group/**/*.scss\" --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
