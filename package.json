{
  "name": "ngx-form-fields",
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint ./projects",
    "lint:fix": "eslint ./projects --fix",
    "test": "npm run test:esm -- --silent",
    "test:coverage": "npm run test:esm -- --silent --collectCoverage",
    "test:esm": "node --experimental-vm-modules --no-warnings ./node_modules/jest/bin/jest.js",
    "test:local": "npm run test:esm",
    "build:lib:ngx-form-fields": "ng build @anedomansky/ngx-form-fields --configuration=production",
    "build:lib:ngx-form-fields:watch": "ng build @anedomansky/ngx-form-fields --configuration development --watch",
    "test:lib:ngx-form-fields": "npm run test:esm -- -c=jest.ngx-form-fields.config.ts --silent",
    "test:lib:ngx-form-fields:local": "npm run test:esm -- -c=jest.ngx-form-fields.config.ts",
    "start:app:test-app": "ng serve test-app",
    "build:app:test-app": "ng build test-app",
    "test:app:test-app": "npm run test:esm -- -c=jest.test-app.config.ts --silent",
    "test:app:test-app:local": "npm run test:esm -- -c=jest.test-app.config.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~18.2.10",
