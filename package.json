    "eslint-plugin-simple-import-sort": "10.0.0",
    "jest": "29.5.0",
    "jest-preset-angular": "13.1.1",
    "ng-packagr": "^16.0.0",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-standard": "29.0.0",
