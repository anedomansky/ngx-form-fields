    "jest-environment-jsdom": "~29.7.0",
    "jest-preset-angular": "~14.2.4",
    "ng-packagr": "~18.2.1",
    "prettier": "~3.3.3",
    "stylelint": "~16.10.0",
    "stylelint-config-sass-guidelines": "~12.1.0",
    "stylelint-config-standard-scss": "~13.1.0",
